{
  "name": "extension-configuration-starter",
  "version": "0.0.0",
  "description": "Starter Kit for Extension Configurations",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "bullhorn": "bullhorn",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "npm run tslint -- \"src/**/*.ts\" --format verbose --type-check --project tsconfig.json",
    "extract": "npm run bullhorn -- extensions extract",
    "upload": "npm run bullhorn -- extensions upload"
  },
  "dependencies": {
    "@bullhorn/bullhorn-cli": "^2.0.0",
    "rimraf": "^2.6.2",
    "tslint": "~5.10.0",
    "typescript": "~2.9.2"
  }
}
